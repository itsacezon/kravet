            i.icon-cross.js-marine__product-deselect
            .button__group
              a.button.marine__more-info href="#0" Info
              a.button.marine__buy href="#0" Buy
        .card__body
          .card__header
            h6.marine__name = @fish[0]
            h6.marine__price â‚±#{rand(100..1000)}
